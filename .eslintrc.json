{
  "plugins": ["prettier", "@typescript-eslint"],
  "extends": ["airbnb-base", "airbnb-typescript/base", "eslint:recommended"],
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": "off",
    "no-console": 0
  }
}
